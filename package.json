{
  "name": "timed",
  "version": "2.1.3",
  "private": true,
  "description": "A time tracking software written in Ember.js",
  "repository": "https://github.com/adfinis-sygroup/timed-frontend",
  "license": "AGPL-3.0",
  "author": "Adfinis AG <info@adfinis.com>",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint . --config-path .template-lintrc-ci.js",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint --config .eslintrc.js .",
    "lint:js:fix": "eslint --config .eslintrc.js . --fix",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "start": "ember server --proxy http://localhost:8000",
    "test": "npm-run-all test:*",
    "test:ember": "ember test"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --config .eslintrc.js --cache --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.{scss,graphql,json,md,yml}": "prettier --write"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  },
